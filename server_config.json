{
    "default":{
        "init_plugin_on_start": true
    },
    "grpc": {
        "port": 50051
    },
    "elasticsearch": {
        "host": "localhost",
        "port": 9200,
        "index": "iart",
        "suggester": "suggester"
    },
    "features": [
        {
            "type": "ImageNetInceptionFeature",
            "output": "image_net_inception",
            "params": {
                "host": "localhost",
                "port": 6379,
                "model_name": "imagenet_inception",
                "model_device": "gpu",
                "model_file": "/nfs/data/iart/web/models/web/imagenet_inception/imagenet_inception.pb",
                "pca_model_name": "imagenet_inception_pca_128",
                "pca_model_file": "/nfs/data/iart/web/models/web/imagenet_inception/pca_128.onnx",
                "multicrop": true,
                "max_dim": null,
                "min_dim": 244
            }
        },
        {
            "type": "YUVHistogramFeature",
            "output": "yuv_histogram",
            "params": {
                "host": "localhost",
                "port": 6379,
                "model_name": "yuv_histogram",
                "model_file": "/nfs/data/iart/web/models/web/yuv_histogram/yuv_histogram.pt",
                "max_dim": 128,
                "min_dim": 244
            }
        },
        {
            "type": "ByolEmbeddingFeature",
            "output": "byol_wikipedia",
            "params": {
                "host": "localhost",
                "port": 6379,
                "model_name": "byol_wikipedia",
                "model_file": "/nfs/data/iart/web/models/web/byol_embedding/byol_wikipedia_gpu.pt",
                "model_device": "gpu"
            }
        }
    ],
    "classifiers": [
        {
            "type": "KaggleDenseClassifier",
            "output": "image_net_inception",
            "params": {
                "host": "localhost",
                "port": 6379,
                "model_name": "kaggle_densenet",
                "model_device": "gpu",
                "model_file": "/nfs/data/iart/web/models/web/kaggle_painter/kaggle_densenet.pb",
                "mapping_file": "/nfs/data/iart/web/models/web/kaggle_painter/kaggle_mapping.json",
                "multicrop": true,
                "max_dim": null,
                "min_dim": 244,
                "threshold": 0.5
            }
        },
        {
            "type": "ImageNetResnetClassifier",
            "output": "image_net_resnet_inception",
            "params": {
                "host": "localhost",
                "port": 6379,
                "model_name": "imagenet_resnet",
                "model_device": "gpu",
                "model_file": "/nfs/data/iart/web/models/web/imagenet_resnet/imagenet_resnet_gpu.pt",
                "mapping_file": "/nfs/data/iart/web/models/web/imagenet_resnet/imagenet_mapping.json",
                "multicrop": true,
                "max_dim": null,
                "min_dim": 244,
                "threshold": 0.5
            }
        }
    ]
}